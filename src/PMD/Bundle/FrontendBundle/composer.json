{
    "name": "piotrminkina/frontend-bundle",
    "license": "MIT",
    "type": "symfony-bundle",
    "description": "Symfony2 Frontend Bundle that helps build User Interfaces by provide set of Blocks and Layouts to use with existing standard controllers",
    "authors": [
        {
            "name": "Piotr Minkina",
            "email": "projekty@piotrminkina.pl",
            "homepage": "http://www.piotrminkina.pl"
        }
    ],
    "autoload": {
        "psr-0": { "PMD\\Bundle\\FrontendBundle": "" }
    },
    "require": {
        "php": ">=5.3.3",
        "symfony/http-kernel": "~2.5",
        "symfony/config": "~2.5",
        "symfony/dependency-injection": "~2.5",
        "symfony/templating": "~2.5",
        "sensio/framework-extra-bundle": "~3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~3.7"
    },
    "suggest": {
        "twig/twig": "~1.16"
    },
    "config": {
        "bin-dir": "bin"
    },
    "target-dir": "PMD/Bundle/FrontendBundle",
    "extra": {
        "branch-alias": {
            "dev-master": "0.1-dev"
        }
    }
}
