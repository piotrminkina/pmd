{
    "name": "piotrminkina/doctrine-bundle",
    "license": "MIT",
    "type": "symfony-bundle",
    "description": "Symfony2 Doctrine Bundle that adds simple improvements to standard Doctrine Bundle",
    "authors": [
        {
            "name": "Piotr Minkina",
            "email": "projekty@piotrminkina.pl",
            "homepage": "http://www.piotrminkina.pl"
        }
    ],
    "autoload": {
        "psr-0": { "PMD\\DoctrineBundle": "" }
    },
    "require": {
        "php": ">=5.3.3",
        "symfony/http-kernel": "~2.5",
        "symfony/config": "~2.5",
        "symfony/dependency-injection": "~2.5",
        "doctrine/orm": "~2.2,>=2.2.3"
    },
    "require-dev": {
        "phpunit/phpunit": "~3.7"
    },
    "suggest": {
        "doctrine/doctrine-bundle": "~1.2"
    },
    "config": {
        "bin-dir": "bin"
    },
    "target-dir": "PMD/DoctrineBundle",
    "extra": {
        "branch-alias": {
            "dev-master": "0.1-dev"
        }
    }
}
